<body id="welcome">
    <%- include('header', {}); %>

        <!-- start banner Area -->
	<section class="banner-area" id="home">
		<div class="container">
			<div class="row fullscreen d-flex align-items-center justify-content-start">
				<div class="banner-content col-lg-7">
					<h6 class="text-white text-uppercase">Now you can feel the Energy</h6>
					<h1>
						Start your day with <br>
						coffee!
					</h1>
					<a href="#coffee" class="primary-btn text-uppercase">Buy Now</a>

				</div>
			</div>
		</div>
	</section>
	
	<section class="video-sec-area pb-100 pt-40" id="about">
		<div class="container">
			<div class="row justify-content-start align-items-center">
				<div class="col-lg-6 video-right justify-content-center align-items-center d-flex">
					<div class="overlay overlay-bg"></div>
					<a class="play-btn" href="https://www.youtube.com/watch?v=ARA0AxrnHdM"><img class="img-fluid"
							src="img/play-icon.png" alt=""></a>
				</div>
				<div class="col-lg-6 video-left">
					<h6>Live Coffee making process.</h6>
					<h1>We Telecast our <br>
						Coffee Making Live</h1>
					<p><span>We are here to listen from you deliver exellence</span></p>
					<p>
						Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod temp or incididunt ut
						labore et dolore magna aliqua. Ut enim ad minim.
					</p>
					<img class="img-fluid" src="img/signature.png" alt="">
				</div>
			</div>
		</div>
	</section>
	<!-- End video-sec Area -->

    <!-- Start menu Area -->
	<section class="menu-area section-gap" id="coffee">
		<div class="container">
			<div class="row d-flex justify-content-center">
				<div class="menu-content pb-60 col-lg-10">
					<div class="title text-center">
						<h1 class="mb-10">What kind of Coffee we serve for you</h1>
						<p>Who are in extremely love with eco friendly system.</p>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<table class="table">
						<tbody>
							<% for (var i = 0; i < product.length; i+=4) { %>
								<tr>
									<% for (var j = i; j < i+4; j++) { %>
										<% if (j < product.length) { %>
											<td>
												<a href="/products/<%- product[j].idProd %>" style="color: brown; text-decoration: none;">
													<div class="single-menu">
														<div class="title-div justify-content-between d-flex">
															<h4><%- product[j].nameProd %></h4>
															<p class="price float-right">
																<%- product[j].priceProd %>$
															</p>
														</div>
														<p>
															<%- product[j].descProd %>
														</p>
													</div>
												</a>
											</td>
										<% } %>
									<% } %>
								</tr>
							<% } %>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</section>
	
	<!-- End menu Area -->

